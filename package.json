{
  "name": "google-translate-api-browser",
  "version": "3.0.1",
  "description": "A free and unlimited API for Google Translate that works in browser",
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/cjvnjde/google-translate-api-browser"
